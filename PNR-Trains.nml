  model_life: VEHICLE_NEVER_EXPIRES;
  vehicle_life: 60;
  retire_early: 5;
  loading_speed: 0;
  cost_factor: 25;
  speed: 0 km/h;
  power: 0 hp;
  cargo_capacity: 1;
  weight: 0 ton;
  tractive_effort_coefficient: 0.3;
  air_drag_coefficient: 0;

  refittable_cargo_classes: bitmask(CC_PASSENGERS, CC_MAIL, CC_ARMOURED); non_refittable_cargo_classes: 0;
  misc_flags: bitmask(TRAIN_FLAG_2CC);

  track_type: RAIL;


  visual_effect_and_powered: visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);

  sprite_id: SPRITE_ID_NEW_TRAIN;
  dual_headed: 0;
  length: 8;

   }
   graphics {
      purchase: set_pnr_mu_coach_purchase;
   }
}
