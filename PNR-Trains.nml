/*Kiha 35*/
item (FEAT_TRAINS, item_dmu_kiha_35) {
   property {
      name: string(STR_DMU_KiHa_35);
      climates_available: bitmask(CLIMATE_TEMPERATE);
  introduction_date: date(2015, 1, 1);
  model_life: 30;
  vehicle_life: 60;
  retire_early: 5;
  loading_speed: 40;
  cost_factor: 25;
  speed: 80 km/h;
  power: 250 hp;
  cargo_capacity: 250;
  weight: 129 ton;
  tractive_effort_coefficient: 0.3;
  air_drag_coefficient: 0;

  reliability_decay: 20;
  refittable_cargo_classes: bitmask(CC_PASSENGERS, CC_MAIL, CC_ARMOURED); non_refittable_cargo_classes: 0;
  misc_flags: bitmask(TRAIN_FLAG_2CC, TRAIN_FLAG_MU);
  refit_cost: 0;
  track_type: RAIL;

  running_cost_base: RUNNING_COST_DIESEL;
  engine_class: ENGINE_CLASS_DIESEL;
  visual_effect_and_powered: visual_effect_and_powered(VISUAL_EFFECT_DIESEL, 0, DISABLE_WAGON_POWER);

  sprite_id: SPRITE_ID_NEW_TRAIN;
  dual_headed: 1;
  length: 8;

   }

   graphics {
      purchase: set_kiha_35_purchase;
      default: switch_dmu_kiha_35_reversed;

   }

   livery_override (item_pnr_mu_coach) {
     loading_speed: 40;
     cargo_capacity: 250;
  default: switch_kiha_35_pass_cargo;
   }

}
